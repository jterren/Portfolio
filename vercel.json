{
	"headers": [
		{
			"source": "/build/(.*).(gz|br)",
			"headers": [
				{
					"key": "Content-Encoding",
					"value": "gzip"
				},
				{
					"key": "Cache-Control",
					"value": "public, max-age=31536000, immutable"
				}
			]
		},
		{
			"source": "/build/(.*)",
			"headers": [
				{
					"key": "Cache-Control",
					"value": "public, max-age=31536000, immutable"
				}
			]
		}
	],
	"build": {
		"env": {
			"R2_ACCOUNT_ID": "@r2-account-id",
			"R2_ACCESS_KEY_ID": "@r2-access-key",
			"R2_SECRET_ACCESS_KEY": "@r2-secret-key"
		}
	}
}
